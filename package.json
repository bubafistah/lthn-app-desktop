    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mousetrap": "^1.6.5",
    "ng-in-viewport": "^6.1.5",
    "ng-terminal": "^5.0.0-beta.2",
    "ngrx-store-localstorage": "^12.0.1",
    "ngx-moment": "^6.0.2",
    "normalize.css": "^8.0.0",
    "openpgp": "^5.0.0",
